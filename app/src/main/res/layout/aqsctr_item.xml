<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.9"
        android:orientation="vertical">

        <TextView
            android:id="@+id/aqsctr_item_investname"
            style="@style/TextStyle" />
        <TextView
            android:id="@+id/aqsctr_item_monthvalue"
            style="@style/TextStyle" />

        <TextView
            android:id="@+id/aqsctr_item_investypename"
            style="@style/TextStyle" />

        <TextView
            android:id="@+id/aqsctr_item_usemoney"
            style="@style/TextStyle" />

        <TextView
            android:id="@+id/aqsctr_item_qyname"
            style="@style/TextStyle" />

    </LinearLayout>


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_weight="0.1"
        android:gravity="center">

        <include layout="@layout/right_arrow"></include>

    </LinearLayout>


</LinearLayout>
